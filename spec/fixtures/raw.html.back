var setVtickerCurrentIdx = function(idx, cnt) {
    var i = idx >= 10 ? 0 : idx;
    $('#rank_list_open .top_lst > ol > li').children('a').each(function(){
        $(this).css('font-weight','normal');
    });
    if (cnt > 1) {
        for (var j=1;j<=cnt;j++) {
            $('#rank_list_open .top_lst > ol > .rnk'+(i+j)+' > a').css('font-weight','bold');
        }
    }
    else
        $('#rank_list_open .top_lst > ol > .rnk'+(i+1)+' > a').css('font-weight','bold');
}
var initRollingPopularKeyword = function() {
    $('#rank_list').clone().appendTo('#rank_list_open');
    $('#rank_list_open .top_lst > ol > li').children('a').each(function(){
        $(this).css('font-weight','normal');
    });
    $('#rank_list_open .top_lst > ol > .rnk1 > a').css('font-weight','bold');
    $('#rank_list').vTicker('init', { 
        speed: 500,
        pause: 4000,
        showItems: 1,
        mousePause: false,
        animation: true
    });
}
$('.head_srch_kwir').bind('mouseenter', function(){
    $('#rank_list').vTicker('pause', true);
    $('#rank_list').hide();
    $('#rank_list_open').show();
    $(this).addClass('open');
    $(this).bind('mouseleave ', function(){
        $('#rank_list').vTicker('pause', false);
        $('#rank_list_open').hide();
        $('#rank_list').show();
        $(this).removeClass('open');
    });
});
initRollingPopularKeyword();
