<script type="text/javascript">// 최근 검색어
(function(window, document) { // localStorage for IE7 and older
  "use strict";
  var userData, attr, attributes;
  if (!window.localStorage && (userData = document.body) && userData.addBehavior) {
    if (userData.addBehavior("#default#userdata")) {
      userData.load((attr = "localStorage"));
      attributes = userData.XMLDocument.documentElement.attributes;
      window.localStorage = {
        "length" : attributes.length,
        "key" : function(idx) { return (idx >= this.length) ? null : attributes[idx].name; },
        "getItem" : function(key) { return userData.getAttribute(key); },
        "setItem" : function(key, value) {
          userData.setAttribute(key, value);
          userData.save(attr);
          this.length += ((userData.getAttribute(key) === null) ? 1 : 0);
        },
        "removeItem" : function(key) {
          if (userData.getAttribute(key) !== null) {
            userData.removeAttribute(key);
            userData.save(attr);
            this.length = Math.max(0, this.length - 1);
          }
        },
        "clear" : function() {
          while (this.length) { userData.removeAttribute(attributes[--this.length].name); }
          userData.save(attr);
        }
      };
    }
  }
})(this, this.document);

var latest_max_limit = 200;
var setLocalStorage = function(search_keyword){
    // delimeter info => @ : ^at# , | : ^div#
    var search_history = localStorage['search_history'];
    var today = new Date();
    var m = (today.getMonth() + 1).toString();
    var d = (today.getDate()).toString();
    var sh = search_keyword.replace('@', '^at#').replace('|', '^div#') + '@' + (m.length > 1 ? m : '0' + m) + '.' + (d.length > 1 ? d : '0' + d) + '|';
    if (search_history) {
        var sh_arr = search_history.split('|');
        for(var i in sh_arr) {
            if (!sh_arr[i]) continue;
            if (i == latest_max_limit) break;
            var this_term = sh_arr[i].split('@')[0].replace('^at#', '@').replace('^div#', '|');
            if (this_term != search_keyword)
                sh += sh_arr[i] + '|';
        }
    }
    localStorage['search_history'] = sh;
}
var removeLocalStorage = function(id, sh_term){
    var sh = '';
    var search_history = localStorage['search_history'];
    if (search_history) {
        var sh_arr = search_history.split('|');
        for(var i in sh_arr) {
            if (!sh_arr[i]) continue;
            if (sh_arr[i] != sh_term)
                sh += sh_arr[i] + '|';
        }
    }
    localStorage['search_history'] = sh;
    if (id == 'g') doAutoComplete($('#search_keyword').val());
    else if (id == 's') doSrchAutoComplete($('#srch_keyword').val());
}

// 검색
var goSearch = function() {
    var search_keyword = $('#search_keyword').val();
    if (search_keyword == '원하는 상품을 검색해 보세요') {
        $('#search_keyword').val('').focus();
        alert("검색어를 입력해 주세요");
    }
    else {
        if ($('#autocomplete').find('.selected').text() == search_keyword)
            $('#st').val('1');
        else
            $('#st').val('0');

        setLocalStorage(search_keyword);// 최근 검색어 저장
        document.search_form.action = "/search/";
        document.search_form.keyword.value = search_keyword;
        document.search_form.submit();
    }
    return false;
}
var goSearchExp = function(keyword) {
    setLocalStorage(keyword);
    document.location.href='/search/?keyword=' + keyword;
}

// 검색어 자동완성
var AC = null;
var selectedAcItem = 0;
var autocompleteCache = [];
var disp_terms = [];

var selectAcTerm = function(idx) {
    selectedAcItem = idx;
    if (selectedAcItem === null) {
        $('#autocomplete').hide();
        return;
    }

    if (selectedAcItem < 0)
        selectedAcItem = 0;
    if (selectedAcItem >= $('#autocomplete').find('li').length)
        selectedAcItem = $('#autocomplete').find('li').length - 1;

    $('#autocomplete').find('li').removeClass('selected').eq(selectedAcItem).addClass('selected');
    var term = $('#autocomplete').find('.selected>a>.term').text();
    $('#search_keyword').val(term);
    $('#autocomplete').find('.selected').focus();
}

var doAutoComplete = function(word) {
    if (word == "") {
        $('#search_keyword').val('').focus();
        return;
    }
    if (autocompleteCache[word])
        displayAutocompleteList(autocompleteCache[word], word);
    else {
        $.getJSON('/search/getAutoCompleteWords', {keyword: word}, function(resp){
            autocompleteCache[word] = resp;
            displayAutocompleteList(resp, word);
        });
    }
}

var displayAutocompleteList = function(data, search_keyword)  {
    AC.empty();
    selectAcTerm(null);
    try {
        srch_AC.empty();
        srch_selectAcTerm(null);
    }catch(e){}
    var cnt = 0;
    // 최근 검색어 처리
    var search_history = localStorage['search_history'];
    if (search_history) {
        var sh_arr = search_history.split('|');
        for(var i in sh_arr) {
            if (cnt == 2) break;
            if (!sh_arr[i]) continue;
            var tmp_recent = sh_arr[i].split('@');
            var this_term = tmp_recent[0].replace('^at#', '@').replace('^div#', '|');
            if (this_term.indexOf(search_keyword) >= 0) {
                $('<li id="latest_g_' + cnt + '" class="latest"></li>').html('<a tl:area="SACG" title="' + this_term + '" tl:linkord="' + cnt + '" href="#" onclick="javascript:goSearchExp(\'' + this_term + '\');"><span class="term break_word">' + this_term.replace(search_keyword, '<strong>' + search_keyword + '</strong>') + '</span><span class="sdate">' + tmp_recent[1] + '</span></a><a id="latest_g_del_' + cnt + '" class="btn_del" onclick="javascript:removeLocalStorage(\'g\', \'' + sh_arr[i] + '\');">최근검색어 삭제</a>').appendTo(AC);
                disp_terms[cnt] = this_term;
                cnt++;
            }
        }
    }
    $.each(data, function(k, v){ 
        if (cnt <= 10) {
            if (v.type == 'line')
                $('<p class="line"></p>').appendTo(AC);
            else {
                if (!in_array(v.word, disp_terms)) {
                    if (v.type == 'words')
                        $('<li></li>').html('<a tl:area="SACG" tl:linkord="' + cnt + '" href="#" onclick="javascript:goSearchExp(\'' + v.word + '\');"><span class="term">' + v.html + '</span></a>').appendTo(AC);
                    else
                        $('<p></p>').html('<a tl:area="SACG" tl:linkord="' + cnt + '" href="/deallist/'+v.link+'">' + v.html + '</a>').appendTo(AC);
                }
            }
        }
        cnt++;
    });

    if (cnt > 0) {
        selectedAcItem = -1;
        $('#autocomplete').show();
    }
    else
        selectAcTerm(null);
}

AC = $('<ul class="uio_auto_keyword" id="autocomplete"></ul>').hide().insertAfter('#search_form');
var bTyping = false;
var srch_kwd = null;
var search_interval = null;
var check_search_keyword = function() {
    if (srch_kwd != $('#search_keyword').val())
    {
        srch_kwd = $('#search_keyword').val();
        doAutoComplete(srch_kwd);
    }
};

var ff_focus = function() {
    if ($.browser.mozilla !== true) 
        return ;

    $("#autocomplete").hide();
    if (!bTyping)
    {
        search_interval = setInterval(check_search_keyword, 10);
        bTyping = true;
    }
};

var ff_blur = function() {
    if ($.browser.mozilla !== true) 
        return ;

    if (bTyping)
    {
        clearInterval(search_interval);
        bTyping = false;
    }
};

$('#search_keyword').attr('autocomplete','off').keydown(function(event) {
    if (selectedAcItem !== null && (event.keyCode == 38 || event.keyCode ==40)) {
        if (bTyping) {  
            clearInterval(search_interval);
            bTyping = false;
        }

        if (event.keyCode == 38)
            selectAcTerm(selectedAcItem - 1);
        else if (event.keyCode == 40)
            selectAcTerm(selectedAcItem + 1);
    }
});
$('#search_keyword').attr('autocomplete','off').keyup(function(event) {
    var word = this.value;
    if (!word) {
        AC.empty();
        selectAcTerm(null);
    }
    if (word && (event.keyCode > 40 || event.keyCode == 8))
        doAutoComplete(word);
});

$('body').click(function(e) {
    var target = (e) ? e.target : window.event.srcElement;
