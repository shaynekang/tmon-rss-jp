function getBigBannerSlideStart(cat_srl, banner_count) {
    cat_srl = Number(cat_srl);
    var top_banner_type = 'fixed';

    if (top_banner_type == 'fixed')
        return 1;

    var cookie_info = getCookie('store_bigbanner');
    if (cookie_info == null)
    {
        var bigbanner_cookies = {};
        bigbanner_cookies[cat_srl] = 1;

        setCookie('store_bigbanner', $.stringify(bigbanner_cookies) , {path : '/'});

        return 1;
    }

    var bigbanner_cookies = $.parseJSON(cookie_info);
    var last_idx = bigbanner_cookies.length;
    var order = 0;

    for (var i in bigbanner_cookies)
    {
        if (i == cat_srl)
        {
            var val = bigbanner_cookies[i];
            if (bigbanner_cookies[i] < banner_count)
                order = val + 1;
            else
                order = 1;

            bigbanner_cookies[i] = order;
        }
    }

    if (order == 0) // 해당 카테고리의 쿠키값이 없을 경우
    {
        bigbanner_cookies[cat_srl] = 1;
        setCookie('store_bigbanner', $.stringify(bigbanner_cookies), {path : '/'});
        return 1;

    }
    else
    {
        setCookie('store_bigbanner', $.stringify(bigbanner_cookies), {path : '/'});
        return order;
    }
}

