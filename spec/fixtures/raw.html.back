            cache: false
        });
        $.post(url, function(response){
            var res = $.parseJSON(response);
            if ($("#alarm_cancel").css('display') == 'none' || $("#alarm_area").css('display') == 'block')
            {
                var display_keyword = 'block'; 
            }
            else
            {
                var display_keyword = 'none';
