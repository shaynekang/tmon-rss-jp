function validation_head() {
    var mobilestr = /01[016789]/;  
    var m = document.frm_mobile;
    if (!$("#receiveHp").val()) {
        alert("받는번호를 입력해 주세요.");
        $("#receiveHp").focus();
        return;
    }
    
    var text = $('#receiveHp').val();
    var lng = text.length; 
    
    if(lng < 10){
        alert("휴대폰 번호를 정확하게 입력해주세요.");
        m.receiveHp.focus();
        return;
    }
    
    if (!m.receiveHp.value.match(mobilestr)){
        alert("휴대폰 번호를 정확하게 입력해주세요.");
        m.receiveHp.focus();
        return;                
    }            

    $("#frm_mobile").attr("action", "/misc/mobile/sms");
    $("#frm_mobile").submit();
}

function onKeyNumber_head(){
    var evCode = ( window.netscape ) ? event.which : event.keyCode ;

    if ( evCode && (evCode  > 47 && evCode  < 58))
    {
    }
    else if (evCode == 13) // enter
    {
        event.returnValue = false;
    }
    else{
        event.returnValue = false;
        alert('숫자만 입력 하세요.');
    }
}

